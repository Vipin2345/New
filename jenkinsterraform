pipeline{
   agent{ label "linux-1"}
   stages{
       stage("AWS Deploy"){
          steps{
                        withCredentials([string(credentialsId: 'access_key', variable: 'AWS_ACCESS_KEY_ID'), string(credentialsId: 'secret_key', variable: 'AWS_SECRET_ACCESS_KEY')]){
                       // this is access and secret key for AWS
                        sh "aws s3 ls"
                      }
    }
   }
 } 
}
                   
                         
                         
