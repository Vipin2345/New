pipeline{
   agent{ label "linux-1"}
   stages{
       stage("AWS Deploy")
          steps{
                withCredentials([
                      string('aws_access_key_id', variable: "access_key")
                      string('aws_secret_access_key', variable: "secret_key)
                    ]){
                       // this is access and secret key for AWS
                        sh "aws s3 ls"
                      }
}
}
}
                   
                         
                         
